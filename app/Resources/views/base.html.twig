<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Trouver un colocation{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
      <div class="gradient_blue">
        <div class="nav-contour">
      <nav class="navbar navbar-expand-lg navbar-light bg-white">
        <a class="navbar-brand text-color" href="{{ path('homepage') }}">Colocax</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link text-color" href="{{ path('fos_user_profile_show') }}">Se connecter</a>
            </li>
            <li class="nav-item">
              <a class="nav-link text-color" href="{{ path('fos_user_registration_register') }}">S'inscrire</a>
            </li>
          </ul>
        </div>
      </nav>
    </div>
      <div class="div_center">
      <div class="espace"></div>

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
        </div>
      </div>
      </div>
    </body>
</html>
